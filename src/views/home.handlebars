<div class="container">
    <h1> {{title}} </h1>
    <table class="table table table-striped">
        <tr>
            <td>titulo</td>
            <td>descripcion</td>
            <td>precio</td>
            <td>imagen</td>
            <td>code</td>
            <td>stock</td>
            <td>id</td>
            <td>status</td>
        </tr>
        {{#each products}}
            <tr>
                <td>{{title}}</td>
                <td>{{description}}</td>
                <td>{{price}}</td>
                <td>{{thumbnail}}</td>
                <td>{{code}}</td>
                <td>{{stock}}</td>
                <td>{{id}}</td>
                <td>{{status}}</td>
            </tr>
        {{/each}}        
    </table>
</div>
